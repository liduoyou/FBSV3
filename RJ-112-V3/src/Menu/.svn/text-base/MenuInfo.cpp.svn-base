#include "MenuInfo.h"h"
#include <QApplication>

MenuInfo::MenuInfo(){
    setDefaultValue();
}

MenuInfo::~MenuInfo(){

}

void MenuInfo::setDefaultValue()
{
    m_menuicon = ":/Main/Title.png";
    m_menulabeltext = "Title.png";
}

void MenuInfo::setIconName(const QString &icon)
{
    m_menuicon = icon;
}

QString MenuInfo::getIconName() const
{
    return m_menuicon;
}

void MenuInfo::setlabelText(const QString &labeltext)
{
    m_menulabeltext = labeltext;
}

QString MenuInfo::getlabelText() const
{
    return m_menulabeltext;
}
